 "html": "<div class=\"ksa-sales-invoice\">\n    {% if letter_head and not no_letterhead %}\n\t    <div class=\"letter-head\">\n\t    \t{{ letter_head }}\n\t    </div>\n    {% endif %}\n    \n    <div style=\"text-align:center;\">\n        <h2 class=\"invoice-heading\">VAT INVOICE&emsp;\u0641\u0627\u062a\u0648\u0631\u0629 \u0636\u0631\u064a\u0628\u064a\u0629</h2>\n    </div>\n    <div class=\"row\">\n    <div class=\"col-xs-4 text-left\">\n      CASH<br>\n     Date:&emsp;{{ doc.get_formatted(\"posting_date\") or '' }}&emsp;{{ doc.get_formatted(\"posting_time\") or '' }}<br/>\n     Customer:&emsp;<b>{{ doc.get_formatted(\"customer\") or '' }}</b><br>\n    </div>\n    <div class=\"col-xs-4 text-center\">\n      <img src =\"{{ doc.ksa_einv_qr }}\">\n    </div>\n    <div class=\"col-xs-4 text-right\">\n     {{ doc.get_formatted(\"name\") or '' }}<br>  \n     {{ doc.get_formatted(\"customer_name_in_arabic\") or '' }}<br>\n     \n    </div>\n  </div>\n  \t<div data-fieldname=\"items\" data-fieldtype=\"Table\">\n\t    <br>\n\t    <table>\n\t        <tr style=\"border-bottom: 1px solid black;border-top: 1px solid;border-collapse: collapse;\">\n\t            <td class=\"text-center\" style=\"width:5%;\">\u202b\u0627\u0644\u062a\u0633\u0644\u0633\u0644\u202c<br></td>\n\t            <td class=\"text-left\">\u202b\u0627\u0644\u0628\u064a\u0627\u0646\u202c<br></td>\n\t            <td class=\"text-center\" style=\"width:10%\">\u202b\u0627\u0644\u0643\u0645\u064a\u0629\u202c<br></td>\n\t            <td class=\"text-center\" style=\"width:13%\">\u202b\u0627\u0644\u0648\u062d\u062f\u0629\u202c \u202b\u0633\u0639\u0631\u202c<br></td>\n\t            <td class=\"text-center\" style=\"width:13%\"> \u0627\u0644\u0636\u0631\u064a\u0628\u0629\u202c<br></td>\n\t            <td class=\"text-center\" style=\"width:15%\">\u202b\u0627\u0644\u062c\u0645\u0627\u0644\u064a\u202c \u202b\u0645\u0628\u0644\u063a\u202c<br></td>\n\t       </tr>\n\t        {% set rownum = { 'idx' : 1 } %}\n\t        {%- for row in doc.items | sort(attribute='print_item_order') -%}\n                <tr>\n                    <td class=\"text-center\" style=\"width: 5%; vertical-align: middle !important\"> {{ rownum.idx }}</td>\n                    <td style=\"width: 30%; vertical-align: middle !important\"> {{ row.item_name }}</td>\n                    <td class=\"text-center\" style=\"width: 10%; vertical-align: middle !important\"> <b>{{ row.qty|int }}</b></td>\n                    <td class=\"text-center\" style=\"width: 10%; vertical-align: middle !important\"> <b>{{ row.rate }}</b></td>\n                    <td class=\"text-center\" style=\"width: 15%; vertical-align: middle !important\"> {{ row.tax_rate or '0' }}%</td>\n                    <td class=\"text-center\" style=\"width: 10%; vertical-align: middle !important\"> {{ row.amount}}</td>\n                </tr>\n                {% if rownum.update({'idx': rownum.idx + 1 }) %} {% endif %}\n            {%- endfor -%}\n\t    </table>\n   </div>\n   <div>\n\t    <table width=100%>\n\t        <tr style=\"border-bottom: black;border-top: 1px solid;border-collapse: collapse;\">\n\t            <td style=\"width:10%\"></td>\n                <td class=\"text-left\" style=\"width:20%\">\n                    <br>\n                </td>\n                <td class=\"text-center\" style=\"width:20%\">\n                    \u0627\u0644\u0645\u0628\u0644\u063a \u0627\u0644\u0625\u062c\u0645\u0627\u0644\u064a<br>\n                </td>\n                <td class=\"text-right\" style=\"width:20%\">\n                    {% if doc.disable_rounded_total %}\n                        <p style=\"font-size:20px;font-weight:bold;\">{{ doc.get_formatted(\"grand_total\") }}</p>\n                    {% else %}\n                        <p style=\"font-size:20px;font-weight:bold;\">{{ doc.get_formatted(\"rounded_total\") }}</p>\n                    {% endif %}\n                </td>\n            </tr>\n\t    </table>\n\t</div>\n</div>",
